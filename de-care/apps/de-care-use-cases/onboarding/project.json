{
    "name": "de-care-use-cases-onboarding-app",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/de-care-use-cases/onboarding/src",
    "prefix": "de-care-use-case-onboarding-app",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:webpack-browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/de-care-use-cases/onboarding",
                "index": "apps/de-care-use-cases/onboarding/src/index.html",
                "main": "apps/de-care-use-cases/onboarding/src/main.ts",
                "polyfills": "apps/de-care-use-cases/onboarding/src/polyfills.ts",
                "tsConfig": "apps/de-care-use-cases/onboarding/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": ["apps/de-care-use-cases/onboarding/src/favicon.ico", "apps/de-care-use-cases/onboarding/src/assets"],
                "styles": ["apps/de-care-use-cases/onboarding/src/styles.scss", "libs/sxm-assets/scss/global.scss", "./node_modules/sxm/assets/dist/css/minified/sxm.min.css"],
                "stylePreprocessorOptions": {
                    "includePaths": ["./node_modules/sxm/assets/src/scss/"]
                },
                "scripts": [],
                "customWebpackConfig": {
                    "path": "apps/de-care-use-cases/onboarding/webpack.config.js"
                },
                "buildOptimizer": false,
                "optimization": false,
                "vendorChunk": true,
                "extractLicenses": false,
                "sourceMap": true,
                "namedChunks": true
            },
            "configurations": {
                "production": {
                    "buildOptimizer": true,
                    "optimization": true,
                    "vendorChunk": false,
                    "extractLicenses": true,
                    "sourceMap": false,
                    "namedChunks": false,
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "20kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/de-care-use-cases/onboarding/src/environments/environment.ts",
                            "with": "apps/de-care-use-cases/onboarding/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all",
                    "customWebpackConfig": {
                        "path": "apps/de-care-use-cases/onboarding/webpack.prod.config.js"
                    }
                },
                "development": {},
                "canada": {
                    "fileReplacements": [
                        {
                            "replace": "apps/de-care-use-cases/onboarding/src/environments/environment.ts",
                            "with": "apps/de-care-use-cases/onboarding/src/environments/environment.canada.ts"
                        }
                    ]
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@nrwl/angular:webpack-server",
            "configurations": {
                "production": {
                    "browserTarget": "de-care-use-cases-onboarding-app:build:production"
                },
                "development": {
                    "browserTarget": "de-care-use-cases-onboarding-app:build:development",
                    "port": 4230
                },
                "canada": {
                    "browserTarget": "de-care-use-cases-onboarding-app:build:canada",
                    "port": 4231
                }
            },
            "defaultConfiguration": "development",
            "options": {
                "host": "0.0.0.0",
                "disableHostCheck": true
            }
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "de-care-use-cases-onboarding-app:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["apps/de-care-use-cases/onboarding/src/**/*.ts", "apps/de-care-use-cases/onboarding/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/de-care-use-cases/onboarding"],
            "options": {
                "jestConfig": "apps/de-care-use-cases/onboarding/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["type:app"]
}
