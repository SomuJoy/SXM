{
    "name": "storybook-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/storybook-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "apps/storybook-e2e/cypress.json",
                "tsConfig": "apps/storybook-e2e/tsconfig.e2e.json"
            },
            "configurations": {
                "production": {}
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["apps/storybook-e2e/src/**/*.ts", "apps/storybook-e2e/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        }
    },
    "tags": ["type:app", "scope:e2e", "scope:legacy"]
}
