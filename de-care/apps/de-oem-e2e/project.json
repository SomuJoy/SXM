{
    "name": "de-oem-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/de-oem-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "apps/de-oem-e2e/cypress.json",
                "tsConfig": "apps/de-oem-e2e/tsconfig.e2e.json",
                "devServerTarget": "de-oem:serve"
            },
            "configurations": {
                "production": {
                    "devServerTarget": "de-oem:serve:production"
                },
                "canada": {
                    "devServerTarget": "de-oem:serve:canada"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["apps/de-oem-e2e/src/**/*.ts", "apps/de-oem-e2e/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        }
    },
    "tags": ["type:app", "scope:e2e", "scope:de-oem"]
}
