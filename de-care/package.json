        "ci:e2e:care": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='care'",
        "ci:e2e:care-checkout-digital": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='care' --tag='@checkoutDigital'",
        "ci:e2e:care-checkout-satellite": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='care' --tag='@checkoutSatellite'",
