        "serve.care.local": "npx nx run cuwi:serve:local",
        "serve.care.ci": "node tools/scripts/serve-by-environment-care.ts",
        "e2e-local:care": "run-p serve.care.local e2e:care",
        "e2e:care": "npx cypress open --project=apps/e2e-de-care",
        "e2e:care-checkout-digital": "npx cypress open --project=apps/e2e-de-care --env tags='@checkoutDigital',filterSpecs=true,omitFiltered=true",
