        "ci:e2e:care-onboarding": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='care' --tag='@onboarding'",
        "ci:e2e:care-retain": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='care' --tag='@retain'",
        "ci:e2e:care-analytics": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='care' --tag='@analytics'",
        "serve.oem.local": "npx nx run de-oem:serve:local",
        "e2e:oem": "npx cypress open --project=apps/e2e-de-oem",
        "ci:e2e:oem": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='oem'",
        "serve.streaming-onboarding.local": "npx nx run de-streaming-onboarding:serve:local",
        "e2e:streaming-onboarding": "npx cypress open --project=apps/e2e-de-streaming-onboarding",
        "ci:e2e:streaming-onboarding": "npx ts-node tools/scripts/cypress-tools/run-tests-headless.ts --app='streaming-onboarding'",
        "e2e:channel-discovery": "npx cypress open --project=apps/channel-discovery-e2e",
        "ci:e2e:channel-discovery": "npx cypress run --project=apps/channel-discovery-e2e",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main"
    },
