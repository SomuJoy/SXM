{
    "name": "shared-storybook-util-helpers",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/shared/storybook/util-helpers/src",
    "prefix": "de-care",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/shared/storybook/util-helpers/src/**/*.ts", "libs/shared/storybook/util-helpers/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/shared/storybook/util-helpers/jest.config.js",
                "tsConfig": "libs/shared/storybook/util-helpers/tsconfig.spec.json",
                "passWithNoTests": true,
                "setupFile": "libs/shared/storybook/util-helpers/src/test-setup.ts"
            }
        }
    },
    "generators": {
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "tags": ["scope:legacy", "scope:deprecated", "scope:storybook-helpers-deprecated"]
}
