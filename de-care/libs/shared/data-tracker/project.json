{
    "name": "data-tracker",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/shared/data-tracker/src",
    "projectType": "library",
    "prefix": "de-care",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/shared/data-tracker/src/**/*.ts", "libs/shared/data-tracker/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/shared/data-tracker/jest.config.js",
                "tsConfig": "libs/shared/data-tracker/tsconfig.spec.json",
                "setupFile": "libs/shared/data-tracker/src/test-setup.ts",
                "passWithNoTests": true
            }
        }
    },
    "generators": {
        "@nrwl/angular:component": {
            "styleext": "scss"
        }
    },
    "tags": ["type:util", "scope:shared-util"]
}
