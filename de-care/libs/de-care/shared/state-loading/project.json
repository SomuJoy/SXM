{
    "name": "de-care-shared-state-loading",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/de-care/shared/state-loading/src",
    "prefix": "de-care",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/de-care/shared/state-loading/src/**/*.ts", "libs/de-care/shared/state-loading/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/de-care/shared/state-loading/jest.config.js",
                "tsConfig": "libs/de-care/shared/state-loading/tsconfig.spec.json",
                "passWithNoTests": true,
                "setupFile": "libs/de-care/shared/state-loading/src/test-setup.ts"
            }
        }
    },
    "generators": {
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "tags": ["type:state", "scope:de-care-use-case-shared-state"]
}
