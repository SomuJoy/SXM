{
    "name": "elements",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/elements/src",
    "projectType": "library",
    "prefix": "sxm",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/elements/src/**/*.ts", "libs/elements/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/elements/jest.config.js",
                "tsConfig": "libs/elements/tsconfig.spec.json",
                "setupFile": "libs/elements/src/test-setup.ts",
                "passWithNoTests": true
            }
        }
    },
    "generators": {
        "@nrwl/angular:component": {
            "styleext": "scss"
        }
    },
    "tags": ["type:feature", "scope:de-elements"]
}
