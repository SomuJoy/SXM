{
    "name": "domains-offers-ui-hero",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/domains/offers/ui-hero/src",
    "prefix": "de-care",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/domains/offers/ui-hero/src/**/*.ts", "libs/domains/offers/ui-hero/src/**/*.html"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/domains/offers/ui-hero/jest.config.js",
                "tsConfig": "libs/domains/offers/ui-hero/tsconfig.spec.json",
                "passWithNoTests": true,
                "setupFile": "libs/domains/offers/ui-hero/src/test-setup.ts"
            }
        }
    },
    "generators": {
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "tags": ["type:ui", "scope:domain-ui"]
}
