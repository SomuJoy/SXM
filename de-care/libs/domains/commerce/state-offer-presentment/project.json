            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/domains/commerce/state-offer-presentment/**/*.ts", "libs/domains/commerce/state-offer-presentment/**/*.html"]
            }
        }
    },
    "tags": ["type:state", "scope:domain-state"]
