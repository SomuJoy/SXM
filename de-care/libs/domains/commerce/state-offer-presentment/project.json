{
    "name": "domains-commerce-state-offer-presentment",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/domains/commerce/state-offer-presentment/src",
    "prefix": "de-care",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/domains/commerce/state-offer-presentment/jest.config.ts",
